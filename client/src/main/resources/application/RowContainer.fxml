<ScrollPane fitToHeight="true" maxWidth="1.7976931348623157E308" style="-fx-background-color: trasparent;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RowContainerController">
   <content>
      <VBox>
         <children>
            <Label fx:id="title" maxHeight="-Infinity" prefHeight="50.0" prefWidth="600.0" styleClass="Label-CategoryTitle" text="Label">
